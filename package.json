{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.4",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "typescript": "^4.7.3"
  },
  "type": "module",
  "engines": {
    "node": ">=0.16"
