{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  },
  "type": "module",
  "engines": {
    "node": ">=0.16"
  },
  "engineStrict": true,
  "license": "MIT",
  "scripts": {
    "build": "node render.js",
    "build-watch": "nodemon render.js -i build -e js,json,html,css,html.inc",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  }
}
